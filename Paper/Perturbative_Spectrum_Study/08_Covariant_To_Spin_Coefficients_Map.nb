Cell["\<\
Coefficient identification from the covariant second variation of Q=I1/I0 to \
spin blocks {c2,c1,cs,cw,cm}.\
\>", "Text",ExpressionUUID->"0b8c771c-6820-4cbf-9300-736d72d598d2"],

Cell[BoxData[
 TagBox[
  RowBox[{"q2", "\[Equal]", 
   RowBox[{
    FractionBox[
     RowBox[{"d2I1", "-", 
      RowBox[{"q", " ", "d2I0"}]}], "I0"], "-", 
    FractionBox[
     RowBox[{"2", " ", "dI0", " ", 
      RowBox[{"(", 
       RowBox[{"dI1", "-", 
        RowBox[{"q", " ", "dI0"}]}], ")"}]}], 
     SuperscriptBox["I0", "2"]]}]}],
  HoldForm]], "Input",ExpressionUUID->"fc1471dd-2363-41aa-9bbd-1163ae425950"],

