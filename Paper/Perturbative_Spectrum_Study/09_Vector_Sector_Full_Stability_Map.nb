   RowBox[{"cV2", "\[Equal]", 
    FractionBox["g1", "c1"]}]}],
  HoldForm]], "Input",ExpressionUUID->"da69b187-8cee-458a-9ca8-36bfc5d668cf"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Ks", "\[Equal]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"cs", ",", "cm"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"cm", ",", "cw"}], "}"}]}], "}"}]}], "&&", 
   RowBox[{"Gs", "\[Equal]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"gs", ",", "gm"}], "}"}], ",", 
      RowBox[{"{", 
