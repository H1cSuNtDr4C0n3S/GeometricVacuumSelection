Cell["05 - Nonlocal Quadratic Mixing Kernel", "Title",ExpressionUUID->"0e99945a-cdeb-4446-b054-55188fa2d938"],

Cell["\<\
Two-field quadratic kernel induced by second variation of Q=I1/I0: explicit \
mixing coefficients and IR scaling.\
\>", "Text",ExpressionUUID->"d80ccb05-7ed2-4dfb-90b8-c20fc724a51d"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"dI0", "\[Equal]", 
    RowBox[{
     RowBox[{"a0", " ", "x"}], "+", 
     RowBox[{"b0", " ", "y"}]}]}], "&&", 
   RowBox[{"dI1", "\[Equal]", 
    RowBox[{
     RowBox[{"a1", " ", "x"}], "+", 
     RowBox[{"b1", " ", "y"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"cc8a5f0e-3cda-4480-a0b9-6559317ef797"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"d2I0", "\[Equal]", 
    RowBox[{
     RowBox[{"aa0", " ", 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{"2", " ", "ab0", " ", "x", " ", "y"}], "+", 
     RowBox[{"bb0", " ", 
      SuperscriptBox["y", "2"]}]}]}], "&&", 
   RowBox[{"d2I1", "\[Equal]", 
    RowBox[{
     RowBox[{"aa1", " ", 
      SuperscriptBox["x", "2"]}], "+", 
     RowBox[{"2", " ", "ab1", " ", "x", " ", "y"}], "+", 
     RowBox[{"bb1", " ", 
      SuperscriptBox["y", "2"]}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"2293a000-ee84-40dd-89d1-3be816630dae"],

Cell[BoxData[
 TagBox[
  RowBox[{"q2", "\[Equal]", 
   RowBox[{
    FractionBox[
     RowBox[{"d2I1", "-", 
      RowBox[{"q", " ", "d2I0"}]}], "i0"], "-", 
    FractionBox[
     RowBox[{"2", " ", "dI0", " ", 
      RowBox[{"(", 
       RowBox[{"dI1", "-", 
        RowBox[{"q", " ", "dI0"}]}], ")"}]}], 
     SuperscriptBox["i0", "2"]]}]}],
  HoldForm]], "Input",ExpressionUUID->"7ac3eb3f-1d3e-47f8-bb1a-f5067334d3e1"],

