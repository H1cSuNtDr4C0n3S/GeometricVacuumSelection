Cell["07 - Spin-Projector Quadratic Operator (4D Proxy)", "Title",ExpressionUUID->"9296571c-cce2-4407-8721-41954a7f1c84"],

Cell["\<\
General tensor/vector/scalar block decomposition for a symmetric spatial \
tensor kernel using Barnes-Rivers projectors; includes IR-suppressed \
non-local scalar mixing.\
\>", "Text",ExpressionUUID->"6c2f071b-af4d-4a3c-9cda-174f6a92d37e"],

Cell[BoxData[
 TagBox[
  RowBox[{"Op", "\[Equal]", 
   RowBox[{
    RowBox[{"c2", " ", "P2"}], "+", 
    RowBox[{"c1", " ", "P1"}], "+", 
    RowBox[{"cs", " ", "P0s"}], "+", 
    RowBox[{"cw", " ", "P0w"}], "+", 
    RowBox[{"cm", " ", 
     RowBox[{"(", 
      RowBox[{"P0sw", "+", "P0ws"}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"893eba81-b62b-48ac-b9aa-65edc84d221c"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"P2", "+", "P1", "+", "P0s", "+", "P0w"}], "\[Equal]", "Isym"}],
  HoldForm]], "Input",ExpressionUUID->"def487cc-5e8c-4c28-ab5a-da99f9eb07af"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Kscalar", "\[Equal]", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"cs", ",", "cm"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"cm", ",", "cw"}], "}"}]}], "}"}]}], "&&", 
   RowBox[{"detKscalar", "\[Equal]", 
    RowBox[{
     RowBox[{"cs", " ", "cw"}], "-", 
     SuperscriptBox["cm", "2"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"14db2a47-539b-4323-9022-b891bec2d03c"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"{", 
