Cell["03 - FRW Scalar Constraint Freeze Map", "Title",ExpressionUUID->"3dd7d3db-a4ab-4a90-a656-4dd9f997caa4"],

Cell["\<\
Map from the linear FRW constraint channel (already derived in \
Variational_Derivation/17) to explicit scalar freezing relations.\
\>", "Text",ExpressionUUID->"dabb8d1c-afe0-4ab7-8092-ca0544ec3751"],

Cell[BoxData[
 TagBox[
  RowBox[{"C1onBranch", "\[Equal]", 
   RowBox[{
    RowBox[{"-", "2"}], " ", 
    RowBox[{"Exp", "[", 
     RowBox[{"3", " ", "H0", " ", "t"}], "]"}], " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["K0", "2"], " ", "n1"}], "-", 
      RowBox[{"3", " ", "H0", " ", "up"}]}], ")"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"3727c794-fec3-44d8-82e2-c99e513e9b4c"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"H0", "\[Equal]", 
    FractionBox["K0", 
     SqrtBox["3"]]}], "&&", 
   RowBox[{"n1", "\[Equal]", 
    FractionBox[
     RowBox[{
      SqrtBox["3"], " ", "up"}], "K0"]}]}],
  HoldForm]], "Input",ExpressionUUID->"78b83212-8072-4edb-a1c4-e06baac12a23"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"n1", "\[Equal]", "0"}], "\[Rule]", 
   RowBox[{"up", "\[Equal]", "0"}]}],
  HoldForm]], "Input",ExpressionUUID->"44fd9076-c91a-4d4d-be81-d4e3bbe00386"],

Cell[BoxData[
 TagBox[
