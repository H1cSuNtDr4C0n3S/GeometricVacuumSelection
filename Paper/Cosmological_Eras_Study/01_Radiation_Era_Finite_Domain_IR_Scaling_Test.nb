Notebook[{
Cell["01 - Radiation Era Finite-Domain IR Scaling Test", "Title",ExpressionUUID->"6bf95794-152b-44f7-8655-44cdc1c6d800"],

Cell["\<\
Objective: in radiation era, verify that a finite causal-domain proxy keeps \
the non-local channel IR-suppressed and with expected H^3 scaling.\
\>", "Text",ExpressionUUID->"b414f9e5-6186-46c8-999e-becba629dadb"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"hNorm", "[", "z_", "]"}], ":=", 
   FractionBox[
    RowBox[{"\[ExponentialE]", "[", "z", "]"}], 
    RowBox[{"\[ExponentialE]", "[", "zRef", "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"9c5ad66f-f512-405c-8d53-972ef936e27c"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Rdom", "[", "h_", "]"}], ":=", 
   FractionBox[
    SqrtBox[
     RowBox[{"1", "-", "z2"}]], "h"]}],
  HoldForm]], "Input",ExpressionUUID->"b8227966-47a0-4308-aae7-b03c54438c66"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", "h_", "]"}], ":=", 
   RowBox[{
    RowBox[{"3", " ", 
     SuperscriptBox["h", "2"]}], "+", 
    FractionBox[
     RowBox[{"I1", "[", "h", "]"}], 
     RowBox[{"I0", "[", "h", "]"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"23b7ea0a-8ad2-478c-a1fd-a4f8176375a8"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaQ", "[", "h_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Q", "[", "h", "]"}], "-", 
    RowBox[{"3", " ", 
     SuperscriptBox["h", "2"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"dfa85a97-5b22-48c2-9976-7f96acc31cf1"]
},
