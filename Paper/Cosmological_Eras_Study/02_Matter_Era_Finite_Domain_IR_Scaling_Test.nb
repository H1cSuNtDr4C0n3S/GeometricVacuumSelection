Notebook[{
Cell["02 - Matter Era Finite-Domain IR Scaling Test", "Title",ExpressionUUID->"2e59009f-fb00-4390-9c5c-3ce78385a2a4"],

Cell["\<\
Objective: in matter era, verify finite-domain non-local channel remains \
IR-suppressed and follows expected scaling when H is reduced.\
\>", "Text",ExpressionUUID->"6c5ababc-40ad-4342-84f8-017ceb78fe90"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"hNorm", "[", "z_", "]"}], ":=", 
   FractionBox[
    RowBox[{"\[ExponentialE]", "[", "z", "]"}], 
    RowBox[{"\[ExponentialE]", "[", "zRef", "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"6ef8b7f5-835d-4bfd-a9e6-912b36a0df66"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Rdom", "[", "h_", "]"}], ":=", 
   FractionBox[
    SqrtBox[
     RowBox[{"1", "-", "z2"}]], "h"]}],
  HoldForm]], "Input",ExpressionUUID->"3b42f2e6-0ac6-41db-8724-82c8de62177c"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", "h_", "]"}], ":=", 
   RowBox[{
    RowBox[{"3", " ", 
     SuperscriptBox["h", "2"]}], "+", 
    FractionBox[
     RowBox[{"I1", "[", "h", "]"}], 
     RowBox[{"I0", "[", "h", "]"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"2d6ebcb3-9c59-4cc9-9180-637ef015beb9"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaQ", "[", "h_", "]"}], ":=", 
   RowBox[{
    RowBox[{"Q", "[", "h", "]"}], "-", 
    RowBox[{"3", " ", 
     SuperscriptBox["h", "2"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"5c3d5b2d-3050-4020-bdb0-3094e587dad9"]
},
