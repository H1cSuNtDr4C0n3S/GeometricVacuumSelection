Notebook[{
Cell["16 - FRW Minisuperspace EOM", "Title",ExpressionUUID->"bcfb1ad3-aa2c-4a67-8689-7d7152cb5866"],

Cell["\<\
Euler-Lagrange equations for (a,N,lambda) in a flat FRW minisuperspace proxy \
with leafwise constraint term.\
\>", "Text",ExpressionUUID->"4f7ad39f-38c8-43df-b927-a974e6f53ac6"],

Cell[BoxData[
 TagBox[
  RowBox[{"L", "=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"3", " ", "a", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         SuperscriptBox["a", "\[Prime]",
          MultilineFunction->None], ")"}], "2"]}], 
      RowBox[{"kappa", " ", "N"}]]}], "-", 
    FractionBox[
     RowBox[{"Lambda", " ", 
      SuperscriptBox["a", "3"], " ", "N"}], "kappa"], "+", 
    RowBox[{"lambda", " ", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"3", " ", "a", " ", 
         SuperscriptBox[
          RowBox[{"(", 
           SuperscriptBox["a", "\[Prime]",
            MultilineFunction->None], ")"}], "2"]}], 
        SuperscriptBox["N", "2"]], "-", 
       RowBox[{
        SuperscriptBox["K0", "2"], " ", 
        SuperscriptBox["a", "3"]}]}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"94ab697e-ab0d-4ada-9468-2db1a3a4a172"],

Cell[BoxData[
 RowBox[{"HoldForm", "[", 
  RowBox[{
   RowBox[{"\[ExponentialE]_a", "=", 
    RowBox[{
     FractionBox["dL", "da"], "-", 
     FractionBox[
      RowBox[{"d", " ", "dL"}], 
      RowBox[{"dt", " ", 
       SuperscriptBox["da", "\[Prime]",
        MultilineFunction->None]}]]}]}], ",", 
   RowBox[{"\[ExponentialE]_N", "=", 
    FractionBox["dL", "dN"]}], ",", 
   RowBox[{"\[ExponentialE]_lambda", "=", 
    FractionBox["dL", "dlambda"]}]}], "]"}]], "Input",ExpressionUUID->\
"0cd25da3-85dc-47b5-b1a0-54d62947ceb1"],

Cell[BoxData[
 RowBox[{"HoldForm", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"deSitter", " ", 
     RowBox[{"(", 
      RowBox[{"branch", ":", "a"}], ")"}]}], "=", 
    SuperscriptBox["e", 
     RowBox[{"H", " ", "t"}]]}], ",", 
   RowBox[{"N", "=", "1"}], ",", 
   RowBox[{
    SuperscriptBox["H", "2"], "=", 
    FractionBox[
     SuperscriptBox["K0", "2"], "3"]}]}], "]"}]], "Input",ExpressionUUID->\
"c8b8066e-bde8-4401-b855-6c7fa7860a0d"]
