Cell["20 - Minisuperspace Noether-Bianchi Identity", "Title",ExpressionUUID->"362c81f4-b66d-4142-a6c8-7851510d3996"],

Cell["\<\
Off-shell gauge identity for time-reparametrization in FRW minisuperspace: \
structural relation among Ea, EN, Elam.\
\>", "Text",ExpressionUUID->"fa8a3e06-e01d-49de-9d0f-2e145d2d9d5e"],

Cell[BoxData[
 TagBox[
  RowBox[{"L", "=", 
   RowBox[{
    RowBox[{"-", 
     FractionBox[
      RowBox[{"3", " ", "a", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         SuperscriptBox["a", "\[Prime]",
          MultilineFunction->None], ")"}], "2"]}], 
      RowBox[{"kappa", " ", "N"}]]}], "-", 
    FractionBox[
     RowBox[{"Lambda", " ", 
      SuperscriptBox["a", "3"], " ", "N"}], "kappa"], "+", 
    RowBox[{"lambda", " ", 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"3", " ", "a", " ", 
         SuperscriptBox[
          RowBox[{"(", 
           SuperscriptBox["a", "\[Prime]",
            MultilineFunction->None], ")"}], "2"]}], 
        SuperscriptBox["N", "2"]], "-", 
       RowBox[{
        SuperscriptBox["K0", "2"], " ", 
        SuperscriptBox["a", "3"]}]}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"4ad1f980-192c-42dd-a846-a9b5dac5a400"],

Cell[BoxData[
 RowBox[{"HoldForm", "[", 
  RowBox[{
   RowBox[{"Ea", "\[Equal]", 
    RowBox[{
     FractionBox["dL", "da"], "-", 
     FractionBox[
      RowBox[{"d", " ", "dL"}], 
      RowBox[{"dt", " ", 
       SuperscriptBox["da", "\[Prime]",
        MultilineFunction->None]}]]}]}], ",", 
   RowBox[{"EN", "\[Equal]", 
    FractionBox["dL", "dN"]}], ",", 
   RowBox[{"Elam", "\[Equal]", 
    FractionBox["dL", "dlambda"]}]}], "]"}]], "Input",ExpressionUUID->\
"7ab35188-9ac2-4146-a533-33ee262d3ba7"],

Cell[BoxData[
 TagBox[
  RowBox[{"noetherId", "\[Equal]", 
   RowBox[{
    RowBox[{"Ea", " ", 
     SuperscriptBox["a", "\[Prime]",
      MultilineFunction->None]}], "+", 
    RowBox[{"EN", " ", 
     SuperscriptBox["N", "\[Prime]",
      MultilineFunction->None]}], "+", 
    RowBox[{"Elam", " ", 
     SuperscriptBox["lambda", "\[Prime]",
      MultilineFunction->None]}], "-", 
    FractionBox[
     RowBox[{"d", " ", 
      RowBox[{"(", 
       RowBox[{"N", " ", "EN"}], ")"}]}], "dt"]}], "\[Equal]", "0"}],
  HoldForm]], "Input",ExpressionUUID->"6c473281-d78d-4ee2-adea-0aea8fbbb315"]
