Notebook[{
Cell["19 - FRW Linearized Full EOM Reduction", "Title",ExpressionUUID->"0f580b0c-f9f0-46dc-9c94-5ebbfde071ab"],

Cell["\<\
Linearized system (Ea, EN, Elam) around de Sitter: eliminate constraint \
variables and obtain the reduced scalar-mode ODE.\
\>", "Text",ExpressionUUID->"09c1bfc5-9b0a-4bf4-b2a1-8dbba3f1262d"],

Cell[BoxData[
 RowBox[{"HoldForm", "[", 
  RowBox[{
   RowBox[{"a", "=", 
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{"H0", " ", "t"}], "]"}], " ", 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{"eps", " ", 
        RowBox[{"u", "[", "t", "]"}]}]}], ")"}]}]}], ",", 
   RowBox[{"N", "=", 
    RowBox[{"1", "+", 
     RowBox[{"eps", " ", 
      RowBox[{"n", "[", "t", "]"}]}]}]}], ",", 
   RowBox[{"lambda", "=", 
    RowBox[{"lam0", "+", 
     RowBox[{"eps", " ", 
      RowBox[{"l1", "[", "t", "]"}]}]}]}]}], "]"}]], "Input",ExpressionUUID->\
"59397574-f807-4484-8dae-37a637d26fe2"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["Elam", "1"], "\[Equal]", "0"}], "\[Rule]", 
   RowBox[{
    RowBox[{"n", "[", "t", "]"}], "\[Equal]", 
    FractionBox[
     RowBox[{
      SuperscriptBox["u", "\[Prime]",
       MultilineFunction->None], "[", "t", "]"}], "H0"]}]}],
  HoldForm]], "Input",ExpressionUUID->"bf9d2a39-4e32-4bc4-9e0f-c089786d8a8d"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{
    RowBox[{
     SuperscriptBox["EN", "1"], "\[Equal]", "0"}], "\[Rule]", 
    RowBox[{"l1", "[", "t", "]"}]}], "=", 
   RowBox[{"l1", "[", 
    RowBox[{"u", ",", 
     SuperscriptBox["u", "\[Prime]",
      MultilineFunction->None]}], "]"}]}],
  HoldForm]], "Input",ExpressionUUID->"32d65948-af13-49c1-a845-93ce70169d91"],

Cell[BoxData[
 TagBox[
  RowBox[{
   SuperscriptBox["Ea", "1"], "\[Rule]", 
   RowBox[{"reduced", " ", "ODE", " ", "for", " ", 
    RowBox[{"u", "[", "t", "]"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"1f728b58-2f25-4441-b26b-6889145ce0fb"]
