Cell["15 - FRW K2 and Leafwise Average", "Title",ExpressionUUID->"9ce0dca8-84bd-4b76-aaab-9e408e60341e"],

Cell["\<\
Homogeneous FRW reduction: K^2, leafwise average Q=I1/I0, and de Sitter \
selection rule.\
\>", "Text",ExpressionUUID->"6c79befd-e4ea-45fd-a3be-4517311a5bc6"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Kij", "\[Equal]", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{"a", " ", "ad"}], ")"}], " ", "deltaij"}], "N"]}], "&&", 
   RowBox[{"Kmixed", "\[Equal]", 
    FractionBox[
     RowBox[{"ad", " ", "deltaij"}], 
     RowBox[{"a", " ", "N"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"bc68b5fc-e238-48e0-97e2-f3ea87437e7c"],

Cell[BoxData[
 TagBox[
  RowBox[{"K2", "\[Equal]", 
   RowBox[{"3", " ", 
    SuperscriptBox[
     RowBox[{"(", 
      FractionBox["ad", 
       RowBox[{"a", " ", "N"}]], ")"}], "2"]}]}],
  HoldForm]], "Input",ExpressionUUID->"d585151d-3a6a-44bb-ac3d-7198102551aa"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", 
    RowBox[{"R", ",", "t"}], "]"}], "\[Equal]", 
   FractionBox["I1", "I0"], "\[Equal]", "K2"}],
  HoldForm]], "Input",ExpressionUUID->"97507b5d-7e42-4683-b8c3-7809d5f8f13e"],

Cell[BoxData[
 TagBox[
  RowBox[{"{", 
   RowBox[{
    RowBox[{"N", "\[Equal]", "1"}], ",", 
    RowBox[{"a", "\[Equal]", 
     RowBox[{"Exp", "[", 
      RowBox[{"H", " ", "t"}], "]"}]}], ",", 
    RowBox[{"Q", "\[Equal]", 
     RowBox[{"3", " ", 
      SuperscriptBox["H", "2"]}]}], ",", 
    RowBox[{
     SuperscriptBox["H", "2"], "\[Equal]", 
     FractionBox[
      SuperscriptBox["K0", "2"], "3"]}]}], "}"}],
  HoldForm]], "Input",ExpressionUUID->"e9289a73-2a76-464f-9dbf-af60d591368a"]
