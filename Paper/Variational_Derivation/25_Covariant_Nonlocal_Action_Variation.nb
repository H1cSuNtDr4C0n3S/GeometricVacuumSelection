Notebook[{
Cell["25 - Covariant Nonlocal Action Variation", "Title",ExpressionUUID->"e723ead2-de5e-4914-a8f3-f415bec663f2"],

Cell["\<\
Leafwise nonlocal action variation in the paper form S_NL = Integral dTheta \
lambda(Theta) [I1 - K0^2 I0], with full bulk+boundary split and IR \
suppression in the normalized-average channel.\
\>", "Text",ExpressionUUID->"8c22caaf-8392-4247-abbb-4c6f0d65c97d"],

Cell[BoxData[
 TagBox[
  RowBox[{"deltaSleaf", "\[Equal]", 
   RowBox[{
    RowBox[{"eta", " ", 
     RowBox[{"(", 
      RowBox[{"I1", "-", 
       RowBox[{
        SuperscriptBox["K0", "2"], " ", "I0"}]}], ")"}]}], "+", 
    RowBox[{"lambda", " ", 
     RowBox[{"(", 
      RowBox[{"deltaI1", "-", 
       RowBox[{
        SuperscriptBox["K0", "2"], " ", "deltaI0"}]}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"49eda454-f18b-4bfa-ae64-df4cf6ebc8b3"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaI0", "\[Equal]", 
    RowBox[{"A0", "+", "B0"}]}], "&&", 
   RowBox[{"deltaI1", "\[Equal]", 
    RowBox[{"A1", "+", "C1", "+", "B1"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"94d7c4cd-503c-4327-9bf5-b46ef26bd523"],

Cell[BoxData[
 TagBox[
  RowBox[{"deltaC", "\[Equal]", 
   RowBox[{"deltaI1", "-", 
    RowBox[{
     SuperscriptBox["K0", "2"], " ", "deltaI0"}]}], "\[Equal]", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"A1", "-", 
      RowBox[{
       SuperscriptBox["K0", "2"], " ", "A0"}], "+", "C1"}], ")"}], "+", 
    RowBox[{"(", 
     RowBox[{"B1", "-", 
      RowBox[{
       SuperscriptBox["K0", "2"], " ", "B0"}]}], ")"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"70ad61e9-a40a-4451-988d-9f3e0a277383"],

Cell[BoxData[
 TagBox[
  RowBox[{"deltaQ", "\[Equal]", 
   FractionBox[
    RowBox[{
     RowBox[{"deltaI1", " ", "I0"}], "-", 
     RowBox[{"I1", " ", "deltaI0"}]}], 
    SuperscriptBox["I0", "2"]]}],
  HoldForm]], "Input",ExpressionUUID->"189dec00-b5ba-444e-b000-7d32d4a3bd07"]
