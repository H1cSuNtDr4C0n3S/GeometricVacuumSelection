Notebook[{
Cell["05 - Geometric K2 Cutoff Killer Test", "Title",ExpressionUUID->"3f6b0be9-bfe8-4638-8b8e-2193fb681312"],

Cell["\<\
Replace K2toy with geometric K2 from crossing foliation T in Kottler and \
rerun the same cutoff no-go logic.\
\>", "Text",ExpressionUUID->"39034f7f-3c44-420e-9a7a-597667a42608"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Theta", "\[Equal]", "T", "\[Equal]", 
    RowBox[{"t", "+", 
     RowBox[{"g", "[", "r", "]"}]}]}], "&&", 
   RowBox[{
    RowBox[{
     SuperscriptBox["g", "\[Prime]",
      MultilineFunction->None], "[", "r", "]"}], "\[Equal]", 
    FractionBox[
     SqrtBox[
      RowBox[{"1", "-", 
       RowBox[{"f", "[", 
        RowBox[{"r", ",", "M", ",", "H"}], "]"}]}]], 
     RowBox[{"f", "[", 
      RowBox[{"r", ",", "M", ",", "H"}], "]"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"d9e09049-1778-4595-9057-25b17eb70b46"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"K2geom", "[", "r_", "]"}], "\[Equal]", 
   RowBox[{"KijKij", "[", 
    RowBox[{
     RowBox[{"gamma", " ", "T"}], ",", 
     RowBox[{"beta", " ", "T"}]}], "]"}], "\[Equal]", 
   RowBox[{
    SuperscriptBox[
     RowBox[{
      SuperscriptBox["v", "\[Prime]",
       MultilineFunction->None], "[", "r", "]"}], "2"], "+", 
    RowBox[{"2", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"v", "[", "r", "]"}], "r"], ")"}], "2"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"4f9aacff-b8b4-4fb2-b1b8-aae2d64e8b17"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"v", "[", "r_", "]"}], "\[Equal]", 
   SqrtBox[
    RowBox[{
     FractionBox[
      RowBox[{"2", " ", "M"}], "r"], "+", 
     RowBox[{
      SuperscriptBox["H", "2"], " ", 
      SuperscriptBox["r", "2"]}]}]]}],
  HoldForm]], "Input",ExpressionUUID->"c7aed238-d999-487d-ae03-a68f82e9b7c7"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{
    RowBox[{"Q", "[", "rmin", "]"}], "\[Equal]", 
    FractionBox[
     RowBox[{"I1", "[", "rmin", "]"}], 
     RowBox[{"I0", "[", "rmin", "]"}]]}], "&&", 
   RowBox[{"r", " ", 
    RowBox[{"in", "[", 
     RowBox[{"rmin", ",", "rout"}], "]"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"9cb08494-d32a-4ca0-9c4c-37818c240838"],

Cell[BoxData[
 TagBox[
  RowBox[{
  "killerRule", 
   "\[Equal]", "\"\<if Q does not converge as rmin -> 0 then geometric NO-GO \
in crossing scenario\>\""}],
  HoldForm]], "Input",ExpressionUUID->"15416ead-659a-4448-87e1-f3cd811988a2"]
},
