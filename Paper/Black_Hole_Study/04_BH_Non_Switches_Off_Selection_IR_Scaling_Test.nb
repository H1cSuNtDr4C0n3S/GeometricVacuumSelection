Notebook[{
Cell["04 - BH Non-Switches-Off Selection (IR Scaling Test)", "Title",ExpressionUUID->"062476be-9177-491c-aace-02c87d849042"],

Cell["\<\
Objective: validate that BH contribution to Q is IR-suppressed and decreases \
as cosmic domain grows (H -> H/2).\
\>", "Text",ExpressionUUID->"354e0c76-0be1-4441-bde1-11ac6b5867fb"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"DTheta", "[", 
    RowBox[{"M_", ",", "H_", ",", "z2_"}], "]"}], "\[Equal]", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"r", ">", "0"}], "|", 
     RowBox[{
      RowBox[{"f", "[", 
       RowBox[{"r", ",", "M", ",", "H"}], "]"}], "\[GreaterEqual]", "z2"}]}], 
    "}"}]}],
  HoldForm]], "Input",ExpressionUUID->"3084c4ef-e8eb-4579-8498-98b6f7b54cf0"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", 
    RowBox[{"M_", ",", "H_"}], "]"}], "\[Equal]", 
   FractionBox[
    RowBox[{"I1", "[", 
     RowBox[{"M", ",", "H"}], "]"}], 
    RowBox[{"I0", "[", 
     RowBox[{"M", ",", "H"}], "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"cce5d405-f19b-4350-86a3-093aeeeab492"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"deltaQ", "[", "H_", "]"}], "\[Equal]", 
   RowBox[{
    RowBox[{"Q", "[", 
     RowBox[{"M", ",", "H"}], "]"}], "-", 
    RowBox[{"Q", "[", 
     RowBox[{"0", ",", "H"}], "]"}]}]}],
  HoldForm]], "Input",ExpressionUUID->"14b68946-05e3-4767-9c51-24e771e43a32"],

Cell[BoxData[
 TagBox[
  RowBox[{"testIRRatio", "\[Equal]", 
   FractionBox[
    RowBox[{"deltaQ", "[", 
     FractionBox["H", "2"], "]"}], 
    RowBox[{"deltaQ", "[", "H", "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"6c9e00af-b2d7-4f44-b411-8783c78210f4"]
},
