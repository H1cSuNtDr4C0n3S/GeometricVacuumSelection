Notebook[{
Cell["03 - No-Go on Singular Interiors (Cutoff Test)", "Title",ExpressionUUID->"f7a50de9-4fac-49ad-ad9c-7694d89ec8a6"],

Cell["\<\
Objective: in crossing foliation, test whether singular core behavior is \
compatible with the constraint on Q=I1/I0 as rmin -> 0.\
\>", "Text",ExpressionUUID->"969ebf62-c21f-4ff7-a0b7-622884df51ea"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Theta", "\[Equal]", "T", "\[Equal]", 
    RowBox[{"t", "+", 
     RowBox[{"g", "[", "r", "]"}]}]}], "&&", 
   RowBox[{
    RowBox[{
     SuperscriptBox["g", "\[Prime]",
      MultilineFunction->None], "[", "r", "]"}], "\[Equal]", 
    FractionBox[
     SqrtBox[
      RowBox[{"1", "-", 
       RowBox[{"f", "[", 
        RowBox[{"r", ",", "M", ",", "H"}], "]"}]}]], 
     RowBox[{"f", "[", 
      RowBox[{"r", ",", "M", ",", "H"}], "]"}]]}]}],
  HoldForm]], "Input",ExpressionUUID->"51e7c346-9d2c-4f3f-87bd-8f2635c5d9b0"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"sqrtGamma", "\[Equal]", 
    RowBox[{
     SuperscriptBox["r", "2"], " ", 
     RowBox[{"Sin", "[", "th", "]"}]}]}], "&&", 
   RowBox[{
    RowBox[{"I0", "[", "rmin", "]"}], "\[Equal]", 
    RowBox[{
     FractionBox["1", "3"], " ", 
     RowBox[{"(", 
      RowBox[{"4", " ", "\[Pi]"}], ")"}], " ", 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["rout", "3"], "-", 
       SuperscriptBox["rmin", "3"]}], ")"}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"74b886cf-0049-48d1-b7ad-4300b79d3a6a"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"K2toy", "[", 
    RowBox[{"r_", ",", "n_"}], "]"}], "\[Equal]", 
   RowBox[{"K2bg", "+", 
    RowBox[{"A", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox["rs", "r"], ")"}], "n"]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"eea5d7b8-d1d2-481a-a8f4-294a1023a403"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"I1", "[", 
    RowBox[{"n", ",", "rmin"}], "]"}], "\[Equal]", 
   RowBox[{"4", " ", "\[Pi]", " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", "rmin", "rout"], 
     RowBox[{
      RowBox[{
       RowBox[{"K2toy", "[", 
        RowBox[{"r", ",", "n"}], "]"}], " ", 
       SuperscriptBox["r", "2"]}], 
      RowBox[{"\[DifferentialD]", "r"}]}]}]}]}],
  HoldForm]], "Input",ExpressionUUID->"18f18553-2563-4374-a5c0-9c79dc59e570"],

Cell[BoxData[
 TagBox[
  RowBox[{
   RowBox[{"Q", "[", 
    RowBox[{"n", ",", "rmin"}], "]"}], "\[Equal]", 
   FractionBox[
    RowBox[{"I1", "[", 
     RowBox[{"n", ",", "rmin"}], "]"}], 
    RowBox[{"I0", "[", "rmin", "]"}]]}],
  HoldForm]], "Input",ExpressionUUID->"17cc4836-c831-401b-a757-09111572ede2"],

Cell[BoxData[
 TagBox[
  RowBox[{"rule", "\[Equal]", "\"\<integrable iff n < 3\>\""}],
  HoldForm]], "Input",ExpressionUUID->"29e473b4-1a62-4d89-92e1-08b0b4ac6397"]
},
